<?xml version="1.0"?>

  <robot name="robot" xmlns:xacro="http://www.ros.org/wiki/xacro">
   
   
   
   
      <!-- BODY DRIVE WHEEL URDF -->
   <link name="base">

       <visual>
   	        <origin rpy="0 0 0" xyz="0 0. 0"/>
          <geometry>
              <box size="0.7 0.7 1.2"/>
         </geometry>
         <material name="grey">
          <color rgba="0.6 0.6 0.6 1"/>
        </material>
       </visual>

       <collision>
        <origin rpy="0 0 0" xyz="0 0. 0"/>
        <geometry>
            <box size="0.7 0.7 1.2"/>
       </geometry>
       </collision>

       <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="1"/>
        <inertia
          ixx="1.0" ixy="0.0" ixz="0.0"
          iyy="1.0" iyz="0.0"
          izz="1.0"/>
      </inertial>

    </link>
    <!-- RIGHT DRIVE WHEEL URDF -->
   <joint name="body_right_wheel" type="revolute">
   	<axis xyz="0 0 1"/>
    	<parent link="base"/>
      <child link="right_wheel"/>
      <origin rpy="1.5707 0 0" xyz="0 -0.4 -0.600"/>
      <limit effort="30" velocity="1.0" lower="-2.2" upper="0.7" />
      
   </joint>
   
   <link name="right_wheel">

       <visual>
   	<origin rpy="0 0 0" xyz="0 0 0"/>
        <geometry>
          <cylinder length="0.1" radius="0.1"/>
        </geometry>
        <material name="Light_grey">
          <color rgba="0.8 0.8 0.8 1"/>
        </material>
      </visual>

      <collision>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <geometry>
          <cylinder length="0.1" radius="0.1"/>
        </geometry>
      </collision>

      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="1"/>
        <inertia
          ixx="1.0" ixy="0.0" ixz="0.0"
          iyy="1.0" iyz="0.0"
          izz="1.0"/>
      </inertial>
  </link>

   <!-- LEFT DRIVE WHEEL URDF -->
    <joint name="body_left_wheel" type="revolute">
